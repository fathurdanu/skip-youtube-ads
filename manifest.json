{
  "name": "Skip Youtube Ads",
  "description": "Skip Youtube ads by press Shift + Q",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "key_event.js"
      ]
    }
  ]
}